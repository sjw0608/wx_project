page{
  background-color: #fff;
}

.search_header {
  margin-top: 10rpx;
  width: 100%;
  height: 40px;
  color: #acacac;
  font-size: 16px;
  padding-left: 30rpx;
  padding-right: 128rpx;
  box-sizing: border-box;
}

.search_header input {
  border: 1px solid #f1f1f1;
  height: 40px;
  padding-left: 60rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-radius: 40rpx;
  line-height: 40rpx;
  position: relative;
}

.search_header .icon-sousuo {
  position: absolute;
  left: 50rpx;
  top: 2%;
  font-size: 18px;
  z-index: 10;
  vertical-align: middle;
}

.search_header .icon-x {
  display: inline-block;
  width: 40px;
  height: 40px;
  position: absolute;
  right: 160rpx;
  top: 14rpx;
  font-size: 18px;
  z-index: 10;
  text-align: center;
  vertical-align: middle;
}

.cancel {
  margin-top: 10rpx;
  width: 126rpx;
  height: 40px;
  position: absolute;
  right: 0;
  top: 0;
  text-align: center;
  font-size: 16px;
  color: #000;
  line-height: 45px;
}

.line_view{
  width: 100%;
  margin-top: 25rpx;
  height: 1px;
  background-color: #f1f1f1;
}

.search-list {
  margin-top: 36rpx;
  overflow: hidden;
}

.search-list text {
  display: inline-block;
  padding: 10rpx 20rpx;
  border-radius: 50rpx;
  background-color: #f5f5f5;
  color: #acacae;
  font-size: 28rpx;
  margin: 15rpx 20rpx 0;
  min-width: 50rpx;
  text-align: center;
}

.hot-search {
  margin-left: 32rpx;
  margin-top: 44rpx;
  color: #bfbfbf;
  font-size: 29rpx;
}

.hot-search icon {
  font-size: 32rpx;
}

.hot-search text {
  margin-left: 10rpx;
}

.recently-search{
  margin-left: 32rpx;
  margin-top: 44rpx;
  color: #BFBFBF;
  font-size: 29rpx;
  position: relative;
}

.recently-search icon{
  font-size: 32rpx;
}
.delet{
  position: absolute;
  right: 50rpx;
  top: 0;
}
.recently-search text{
  margin-left: 10rpx;
}
